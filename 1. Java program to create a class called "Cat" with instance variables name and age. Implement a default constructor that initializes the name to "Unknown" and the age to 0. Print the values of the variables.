public class Cat {
    String name;
    int age;

    public Cat() {
        name = "Unknown";
        age = 0;
    }

    public void displayInfo() {
        System.out.println("Cat Name: " + name);
        System.out.println("Cat Age: " + age);
    }

    public static void main(String[] args) {
        Cat myCat = new Cat();
        myCat.displayInfo();
    }
}
